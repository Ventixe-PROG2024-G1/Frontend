@{
    ViewData["Title"] = "Bookings";
}

<div class="booking-container">
    <section class="bookingstatus-container">
        <div class="bookingstatus-card">
            <div>
            </div>
            <div class="bookingstatus-body">
                <div class="bookingstatus-title">Total Bookings</div>
                <h6 class="bookingstatus-stats">55,000</h6>
            </div>
        </div>
        <div class="bookingstatus-card">
            <div class="bookngstatus-header">
            </div>
            <div class="bookingstatus-body">
                <div class="bookingstatus-title">Total Tickets Sold</div>
                <h6 class="bookingstatus-stats">45,000</h6>
            </div>
        </div>
        <div class="bookingstatus-item">
            <div>

            </div>
            <div class="bookingstatus-card">
                <div class="bookingstatus-title">Total Earnings</div>
                <h6 class="bookingstatus-stats">$275,450</h6>
            </div>
        </div>
    </section>
    <section class="bookingtable-container">
        <header class="bookingtable-header">

        </header>
        <table class="bookingtable">
            <thead>
                <tr class="header-row">
                    <th>Invoice ID</th>
                    <th>Date</th>
                    <th class="name-col">Name</th>
                    <th>Event</th>
                    <th class="category-col">Ticket Category</th>
                    <th class="price-col">Price</th>
                    <th class="qty-col">Qty</th>
                    <th class="ticket-col">Ticket</th>
                    <th class="amount-col">Amount</th>
                    <th>Status</th>
                    <th class="voucher-col">E-Voucher</th>
                </tr>
            </thead>
            <tbody>
                <tr class="body-row">
                    <td class="invoice-desktop-col cell-reference">Invoice</td>
                    <td class="invoice-mobile-col">
                        <div class="bookingtable-multi-cell">
                            <div>Invoice</div>
                            <div class="cell-subrow">Mr. Name</div>
                        </div>
                    </td>
                    <td>
                        <div class="bookingtable-multi-cell">
                            <div>10/21/3000</div>
